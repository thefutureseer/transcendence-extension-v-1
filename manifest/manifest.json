{
  "name": "Transc-exten",
  "version": "1.0",
  "description": "Extension for the world, Greetings",
  "manifest_version": 3,
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab":"index.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs",
    "storage"
  ],

  "background": {
    "service_worker": "js/background.js"
  },

  "icons": { "128":"images/icon.png" }
}